{
  "name": "pingbot",
  "version": "1.0.0",
  "description": "A multi-purpose Discord Bot written in TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/KrammyGod/pingbot"
  },
  "bugs": {
    "url": "https://github.com/KrammyGod/pingbot/issues"
  },
  "homepage": "https://github.com/KrammyGod/pingbot#readme",
  "keywords": [],
  "author": {
    "name": "KrammyGod",
    "url": "https://github.com/KrammyGod"
  },
  "publisher": "KrammyGod",
  "license": "MIT",
  "main": "dist/index",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "collect:hi3": "env-cmd --environments hi3 node dist/modules/collector --enable-source-maps",
    "collect:hsr": "env-cmd --environments hsr node dist/modules/collector --enable-source-maps",
    "collect:gi": "env-cmd --environments gi node dist/modules/collector --enable-source-maps",
    "reset": "node dist/modules/reset_db --enable-source-maps",
    "cookie": "node dist/modules/hoyolab --enable-source-maps",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "lint:fix": "eslint --fix src/**/*.ts",
    "prebuild": "npm run clean:dist",
    "build": "tsc && tsc-alias",
    "deploy": "npm run build && node dist/deployment/deploy-commands.js --enable-source-maps",
    "predownload": "tsc scripts/download_waifus_txt.ts",
    "download": "node scripts/download_waifus_txt.js || exit 0",
    "postdownload": "rimraf scripts/download_waifus_txt.js",
    "preupload": "tsc scripts/upload_waifus_txt.ts",
    "upload": "node scripts/upload_waifus_txt.js || exit 0",
    "postupload": "rimraf scripts/upload_waifus_txt.js",
    "start": "node . --enable-source-maps"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.8.9",
    "@types/pg": "^8.10.7",
    "@types/pg-copy-streams": "^1.2.4",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "cheerio": "^1.0.0-rc.12",
    "chrono-node": "^2.7.0",
    "cookie": "^0.5.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "pg": "^8.11.3",
    "pg-copy-streams": "^6.0.6",
    "pixiv.ts": "github:KrammyGod/pixiv.ts",
    "play-dl": "^1.9.7",
    "sodium-native": "^4.0.4"
  }
}
